<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="10007" id="jp.calil.sabatomap" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="BluetoothStatus">
        <param name="android-package" value="com.twofivefivekb.android.bluetoothstatus.BluetoothStatus" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="LocationManager">
        <param name="android-package" value="com.unarin.cordova.beacon.LocationManager" />
    </feature>
    <preference name="webView" value="org.crosswalk.engine.XWalkWebViewEngine" />
    <preference default="15+" name="xwalkVersion" />
    <preference default="--disable-pull-to-refresh-effect" name="xwalkCommandLine" />
    <preference default="embedded" name="xwalkMode" />
    <preference default="true" name="xwalkMultipleApk" />
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="SplashScreen" value="splash" />
    <icon density="mdpi" src="www/res/icons/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="www/res/icons/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="www/res/icons/android/icon-96-xhdpi.png" />
    <icon density="xxhdpi" src="www/res/icons/android/icon-144-xxhdpi.png" />
    <icon density="xxxhdpi" src="www/res/icons/android/icon-192-xxxhdpi.png" />
    <preference name="BackgroundColor" value="0xFF0068B1" />
    <name>Sabae Library Map</name>
    <description>鯖江図書館マップ</description>
    <author email="contact@calil.jp" href="http://calil.jp">CALIL Inc.</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="default" />
    <preference name="webviewbounce" value="false" />
    <preference name="xwalkVersion" value="15+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
